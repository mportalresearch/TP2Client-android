[net]
download_bw=0
upload_bw=380
firewall_policy=0
mtu=1300
enable_nat_helper=0
download_ptime=20
adaptive_rate_control=0

[sip]
sip_port=-1
sip_random_port=0
guess_hostname=1
contact=sip:unknown@unknown-host
inc_timeout=4500
use_info=0
use_ipv6=0
register_only_when_network_is_up=1
default_proxy=0
auto_net_state_mon=0
keepalive_period=30000
auto_answer_replacing_calls=1
ping_with_options=1
media_encryption_mandatory=0
chat_msg_with_contact=1
allow_out_of_subscribe_presence=1
subscribe_expires=1800
use_rport=1
publish_msg_with_contact=0
refresh_generic_publish=0
refresh_generic_subscribe=0
reuse_authorization=1
verify_server_certs=0

[rtp]
audio_rtp_port=7076
video_rtp_port=9078
audio_jitt_comp=60
video_jitt_comp=60
nortp_timeout=30
download_ptime=20
upload_ptime=20

[sound]
playback_dev_id=
ringer_dev_id=
capture_dev_id=
remote_ring=/data/data/org.linphone/files/ringback.wav
local_ring=/data/data/org.linphone/files/oldphone_mono.wav
dtmf_player_amp=0.1
tone_indications=1

[video]
size=qvga


[misc]
max_calls=100
history_max_size=30
uuid=0